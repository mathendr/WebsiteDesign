<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <h1 class="page-header">Overview</h1>
    <h2 class="sub-header">Current Servers</h2>
    <div class="table-responsive">
        <table class="table table-striped" id = "InfoTable">
            <thead>
                <tr>
                    <th>Server ID</th>
                    <th>Server Name</th>
                    <th>Account Name</th>
                </tr>
            </thead>
            <tbody>

                <% if (ID.length > 0) { %>
                    <% for(i = 0; i < ID.length-1; i++){%>
                    <tr >
                        <% var ref = "/ID/" + ID[i]%>
                        <td width = "15%"><a href = <%= ref %>><%=ID[i]%>   </a></td>
                        <td width = "35%"><a href = "/Program/<%=ProjName[i]%>"><%=ProjName[i]%></a></td>
                        <td><a href = "/Account/<%=AccountName[i]%>"><%=AccountName[i]%></a></td>
                        </tr>
                        <%}%>
                    <%}%>
            </tbody>
        </table>
    </div>
</div>
