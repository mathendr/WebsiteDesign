        <script src = "/chart/sorttable.js"></script>
        <table class="table table-striped sortable" id = "InfoTable">
            <thead>
                <tr>
                    <th>Server Name</th>
                    <th>Location</th>
                    <th>Phase</th>
                </tr>
            </thead>
            <tbody>
                <% if (ItemList.length > 0) { %>
                    <% for(i = 0; i < ItemList.length; i++){%>
                <tr>
                    
                    <td width = "35%"><a a href = "/Program/<%=ItemList[i].ProgramName%>/<%=ItemList[i].AccountName%>/<%=ItemList[i].ID%>"><%=ItemList[i].ProgramName%></a></td>
                    <td width = "20%"><a a href = "/Program/<%=ItemList[i].ProgramName%>/<%=ItemList[i].AccountName%>/<%=ItemList[i].ID%>"><%=ItemList[i].Region%></a></td>
                    <td width = "15%"><a a href = "/Program/<%=ItemList[i].ProgramName%>/<%=ItemList[i].AccountName%>/<%=ItemList[i].ID%>"><%=ItemList[i].Phase%></a></td>
                </tr>
                        <%}%>
                    <%}%>
            </tbody>
        </table>
    </div>
</div>