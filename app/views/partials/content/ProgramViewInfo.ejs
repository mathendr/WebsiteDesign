<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <h1 class="page-header"><%=ItemList[0].ProgramName%></h1>
    <div class="table-responsive">
        <script src = "/chart/sorttable.js"></script>
        <table class="table sortable" id="InfoTable">
            <thead>
                <tr>
                    <td align = "center"><b>Account Name</b></td>
                </tr>
            </thead>
            <tbody>

                <% if (ItemList.length > 0) { %>
                    <% for(i = 0; i < ItemList.length; i++){%>
                        <tr>
                            <td align = "center"><a href="/ID/<%=ItemList[i].ID%>"><%=ItemList[i].AccountName%></a></td>
                        </tr>
                        <%}%>
                            <%}%>
            </tbody>
        </table>
    </div>
</div>
