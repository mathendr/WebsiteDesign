<div class="col-sm-10 col-sm-offset-2 col-md-10 col-md-offset-2 main">
    <div class="col-sm-12  col-md-12">
        <h1 class="contact">
            <%=Contact%>
        </h1>
        <div class="col-sm-5  col-md-5 ">
            <h2>Programs</h2>
            <div class="col-sm-12 col-sm-offset-1 col-md-12 col-md-offset-1">
                <%for(var i = 0; i < Data.length-1;i++){%>
                    <li class = "stuff">
                        <%=Data[i]%>
                    </li>
                    <%}%>
            </div>
        </div>
        <div class="col-sm-5 col-sm-offset-2 col-md-5 col-md-offset-2">
            <h2>Email</h2>
            <div class="col-sm-12 col-sm-offset-1 col-md-12 col-md-offset-1">
                <ul>
                    <li class = "email">
                        <%=Data[Data.length-1]%>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

<style>
    h1,
    h2 {
        color: Black;
        border: 4px solid #00b388;
        text-align: center;
    }
    
    li.email {
        font-size: large;
    }

</style>
